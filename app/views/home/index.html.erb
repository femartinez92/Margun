<div id="logo">
	<%= image_tag("Logo.png")%>
</div>
<div id="carousel">
<div id="slides">
<div id="next"></div>
<div id="prev"></div>
<ul>
<li><%= image_tag("Foto Martillo.png")%></li>
<li><%= image_tag("Foto Eolico.png")%></li>
<li><%= image_tag("Foto Lapiz.png")%></li>
<li><%= image_tag("Foto Mineria.png")%></li>
</ul>
</div>
</div>
<div id = "progress_bar">
	<div id="pbar_outerdiv" style="width: 100px; height: 8px; border: 1px solid grey; z-index: 1; position: relative; border-radius: 5px; -moz-border-radius: 5px;">
	<div id="pbar_innerdiv" style="background-color: #920190; z-index: 2; height: 100%; width: 0%;"></div>
	<div id="pbar_innertext" style="z-index: 3; position: absolute; top: 0; left: 0; width: 100%; height: 100%; color: black; font-weight: bold; text-align: center;"></div>
</div>